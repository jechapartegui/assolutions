{
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "name": "assolutions-front",
  "root": "apps/assolutions-front",
  "sourceRoot": "apps/assolutions-front/src",
  "projectType": "application",
  "prefix": "app",
  "i18n": {
    "sourceLocale": "fr",
    "locales": {
      "en": "apps/assolutions-front/src/locale/messages.en.xlf"
    }
  },
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/assolutions-front",
        "index": "apps/assolutions-front/src/index.html",
        "main": "apps/assolutions-front/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/assolutions-front/tsconfig.app.json",
        "assets": [
          "apps/assolutions-front/src/favicon.ico",
          "apps/assolutions-front/src/assets"
        ],
        "styles": ["apps/assolutions-front/src/styles.css"],
        "scripts": [],
        "localize": ["fr", "en"]
      },
      "configurations": {
        "production": {
          
          "outputHashing": "all",
          "budgets": [
            { "type": "initial", "maximumWarning": "2mb", "maximumError": "3mb" },
            { "type": "anyComponentStyle", "maximumWarning": "2kb", "maximumError": "4kb" }
          ]
        },
        "preprod": {
          "fileReplacements": [
            {
              "replace": "apps/assolutions-front/src/environments/environment.ts",
              "with": "apps/assolutions-front/src/environments/environment.preprod.ts"
            }
          ],
          "outputHashing": "all",
          "budgets": [
            { "type": "initial", "maximumWarning": "2mb", "maximumError": "3mb" },
            { "type": "anyComponentStyle", "maximumWarning": "2kb", "maximumError": "4kb" }
          ]
        },
        "development": {
          "optimization": false,
          "buildOptimizer": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "localize": ["fr"],
          "fileReplacements": [
            {
              "replace": "apps/assolutions-front/src/environments/environment.prod.ts",
              "with": "apps/assolutions-front/src/environments/environment.ts"
            }
          ]
        },
        "development-en": {
          "optimization": false,
          "buildOptimizer": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "localize": ["en"],
          "fileReplacements": [
            {
              "replace": "apps/assolutions-front/src/environments/environment.ts",
              "with": "apps/assolutions-front/src/environments/environment.en.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "buildTarget": "assolutions-front:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "assolutions-front:build:development",
          "port": 2211,
          "proxyConfig": "apps/assolutions-front/proxy.conf.json"
        },
        "development-en": {
          "buildTarget": "assolutions-front:build:development-en",
          "port": 2510,
          "proxyConfig": "apps/assolutions-front/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@nx/angular:extract-i18n",
      "options": {
        "browserTarget": "assolutions-front:build"
      },
      "outputs": ["apps/assolutions-front/src/locale"]
    },
    "test": {
      "executor": "@nx/angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/assolutions-front/tsconfig.spec.json",
        "karmaConfig": "apps/assolutions-front/karma.conf.js",
        "assets": [
          "apps/assolutions-front/src/favicon.ico",
          "apps/assolutions-front/src/assets"
        ],
        "styles": ["apps/assolutions-front/src/styles.css"],
        "scripts": []
      }
    }
  },
  "tags": []
}
