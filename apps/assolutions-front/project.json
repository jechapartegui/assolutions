{
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "name": "assolutions-front",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "src",
  "prefix": "app",
  "i18n": {
    "sourceLocale": {
      "code": "fr",
      "baseHref": "/"
    },
    "locales": {
      "en": {
        "translation": "src/locale/messages.en.xlf",
        "baseHref": "/en/"
      }
    }
  },
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/assolutions",
        "index": "src/index.html",
        "main": "src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "tsconfig.app.json",
        "assets": [
          "src/favicon.ico",
          "src/assets"
        ],
        "styles": [
          "src/styles.css"
        ],
        "scripts": [],
        "localize": [
          "en",
          "fr"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "preprod": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "src/environments/environment.prod.ts",
              "with": "src/environments/environment.preprod.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "localize": [
            "fr"
          ],
          "fileReplacements": [
            {
              "replace": "src/environments/environment.prod.ts",
              "with": "src/environments/environment.ts"
            }
          ]
        },
        "development-en": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "localize": [
            "en"
          ],
          "fileReplacements": [
            {
              "replace": "src/environments/environment.prod.ts",
              "with": "src/environments/environment.en.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production",
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "development": {
          "browserTarget": "assolutions-front:build:development",
          "port": 2211,
          "proxyConfig": "./proxy.conf.json"
        },
        "development-en": {
          "browserTarget": "assolutions-front:build:development-en",
          "port": 2510,
          "proxyConfig": "./proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "assolutions-front:build",
        "outputPath": "src/locale"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "tsconfig.spec.json",
        "assets": [
          "src/favicon.ico",
          "src/assets"
        ],
        "styles": [
          "src/styles.css"
        ],
        "scripts": []
      }
    }
  }
}
