{
 "scripts": {
    "clean": "node -e \"try{require('fs').rmSync('dist',{recursive:true,force:true})}catch(e){}\"",
    "prebuild": "npm run clean && npm run build:shared",
    "build:shared": "tsc -p ../../libs/shared/tsconfig.node.json",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json -v",
    "start": "node dist/main.js",
    "start:dev": "cross-env TS_NODE_PROJECT=tsconfig.dev.json ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/config": "^3.2.0",

    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "typeorm-naming-strategies": "^4.1.0",

    "bottleneck": "^2.19.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",

    "crypto-js": "^4.2.0",
    "file-type": "^16.5.4",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "@types/node": "^20.11.30",
    "@types/express": "^4.17.21",
    "@types/nodemailer": "^7.0.1"
  }
}

